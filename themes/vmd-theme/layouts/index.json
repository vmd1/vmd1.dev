{{- $.Scratch.Add "index" slice -}}
{{- range .Site.RegularPages -}}
    {{- $.Scratch.Add "index" (dict "title" .Title "tags" .Params.tags "categories" .Params.categories "contents" .Plain "permalink" .RelPermalink "date" (.Date.Format "2006-01-02T15:04:05Z07:00") "summary" .Summary) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
